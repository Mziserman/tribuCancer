<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Tribu Cancer{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        {% stylesheets filter="scssphp" output="css/style.css"
        "@AppBundle/Resources/sass/main.scss"
        %}
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {# <link rel="stylesheet" type="text/css" href="{{ asset('css/main.css') }}" /> #}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-1524595-1', 'auto');
          ga('send', 'pageview');

        </script>
    </head>
    <body>
        {% block nav %}{% include 'partials/nav.html.twig' %}{% endblock %}
        {% block headerBanner %}{% include 'partials/header.html.twig' %}{% endblock %}
        {% block body %}{% endblock %}
        {% block footer %}{% include 'partials/footer.html.twig' %}{% endblock %}
        {% javascripts '@AppBundle/Resources/js/*.js' %}
            <script src="{{ asset_url }}" type="text/javascript"></script>
        {% endjavascripts %}
    </body>
</html>
